{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://api.example.com"  // Example environment variable
    },
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
  